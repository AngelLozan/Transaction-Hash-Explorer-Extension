{ "manifest_version": 3, 
  "name": "Blockchairtest", 
  "version": "0.1.0",
  "author":"Scott L",
  "permissions": ["storage", "activeTab", "alarms", "tabs"], 
  "action": 
   { "default_popup": "index.html",
     "default_title": "Blockchairtest",
     "default_icon": "EXODUSblockchair.png"

    },
   "icons":{
    "16": "EXODUSblockchair.png",
    "32": "EXODUSblockchair.png",
    "48": "EXODUSblockchair.png",
    "128": "EXODUSblockchair.png"
  },



  "content_security_policy": {
    "extension_pages" : "script-src 'self'; connect-src https://api.blockchair.com/bitcoin/dashboards/transaction/; object-src 'self'"
 },

  "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+I",
                "mac": "Command+I"
            }
        }
    }
}